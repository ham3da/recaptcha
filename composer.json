{
    "name": "greggilbert/recaptcha",
    "description": "reCAPTCHA Validator for Laravel 5",
	"keywords": ["recaptcha", "captcha", "laravel", "laravel5"],
	"homepage": "https://github.com/ham3da/recaptcha",
	"license": "MIT",
    "abandoned": true,
    "authors": [
        {
            "name": "Greg Gilbert",
            "email": "greg@greg-gilbert.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support":"~10.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Greggilbert\\Recaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Greggilbert\\Recaptcha\\ecaptchaServiceProvider"
            ],
        "branch-alias": {
            "dev-master": "2.3.0"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
